  "devDependencies": {
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.4",
    "prettier": "3.6.2"
  },
  "lint-staged": {
    "*.{cjs,cts,css,html,js,jsx,json,less,md,mdx,mjs,mts,scss,ts,tsx,yaml}": [
      "prettier --write"
    ]
  }
